<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ragger.evilhangman"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <!-- Icon source -->
    <!-- http://www.freepik.com licensed by http://creativecommons.org/licenses/by/3.0/" -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gallow"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ragger.evilhangman.HangmanActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ragger.evilhangman.HighscoresActivity"
            android:label="Highscores"
            android:theme="@style/AppTheme" >
         >
        </activity>
        <activity
            android:name="com.example.ragger.evilhangman.SettingsActivity"
            android:label="Settings"
            android:parentActivityName="com.example.ragger.evilhangman.HangmanActivity"
            android:theme="@style/AppTheme" >
         >
        </activity>
    </application>

</manifest>