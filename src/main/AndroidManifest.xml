<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ragger.evilhangman" >

    <!-- Icon source-->
   <!--http://www.freepik.com licensed by http://creativecommons.org/licenses/by/3.0/"-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gallow"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HangmanActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HighscoresActivity"
            android:label="Highscores"
            android:theme="@style/AppTheme" >
         >
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="Settings"
            android:theme="@style/AppTheme"
            android:parentActivityName=".HangmanActivity">
         >
        </activity>
    </application>

</manifest>
